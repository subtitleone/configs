[
  {
    "type": "regex",
    "match": "[‘’]",
    "replace": "'"
  },
  {
    "type": "regex",
    "match": "[”“]",
    "replace": "\""
  },
  {
    "type": "regex",
    "match": "(\\W?\\s?)\\/([' ]?\\p{L}+)",
    "replace": "$1I$2",
    "description": "Replace / with I"
  },
  {
    "type": "regex",
    "match": "(\\p{L}+)n't 1",
    "replace": "$1n't I",
    "description": "Replace \"*n't 1\" to \"*n't I\""
  },
  {
    "type": "regex",
    "match": "ﬁ",
    "replace": "fi"
  },
  {
    "type": "regex",
    "match": "\\|",
    "replace": "I"
  },
  {
    "type": "regex",
    "match": "(^|\\s)-(\\s?)[1\\[](( ?\\.{2,3}|( ?\\w*)))",
    "replace": "$1-$2I$4",
    "description": "Replace '-1' with '-I'"
  },
  {
    "type": "regex",
    "match": "(^|-| )[l1](t? )",
    "replace": "$1I$2",
    "description": "Replace l with I"
  },
  {
    "type": "regex",
    "match": " 'l ",
    "replace": " 'I "
  },
  {
    "type": "regex",
    "match": "(\\W+)l ",
    "replace": "$1I "
  },
  {
    "type": "regex",
    "match": "(\\W+)iIf ",
    "replace": "$1if "
  },
  {
    "type": "regex",
    "match": "(\\W?)Yeanh(\\W?)",
    "replace": "$1Yeah$2"
  },
  {
    "type": "regex",
    "match": "(\\W?)t00(\\W?)",
    "replace": "$1too$2"
  },
  {
    "type": "regex",
    "match": "(\\W?)itis(\\W?)",
    "replace": "$1it is$2"
  },
  {
    "type": "regex",
    "match": "(\\p{Ll},?) You(\\s?(?!\\p{Ll}))",
    "replace": "$1 you$2",
    "description": "Replace in sentence 'You'"
  },
  {
    "type": "regex",
    "match": "^(\\w+ )?(-? ?)\\[((.(?!\\]))+) ?$",
    "replace": "$1$2I$3",
    "description": "Replace [ with I"
  },
  {
    "type": "regex",
    "match": "(\\w+)''s( |$)",
    "replace": "$1's$2",
    "description": "Fix duplicate single quotes"
  },
  {
    "type": "regex",
    "match": "am 1\\?",
    "replace": "am I?",
    "description": "Fix 'am 1?'"
  }
]